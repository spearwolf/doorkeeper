{
  "port": 6100,
  "tokenStore": "redis",
  "destroyAllTokensAtStartup": false,
  "log": {
    "pretty": false,
    "httpRequests": true
  },
  "redis": {
    "namespace": "doorkeeper"
  },
  "keyFile": {
    "public": "keys/public.pem",
    "private": "keys/private.pem",
    "passphrase": "set as ENV variable DOORKEEPER_KEY_PASSPHRASE"
  }
}
